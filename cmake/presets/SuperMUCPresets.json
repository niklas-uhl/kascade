{
  "version": 9,
  "$schema": "https://cmake.org/cmake/help/latest/_downloads/3e2d73bff478d88a7de0de736ba5e361/schema.json",
  "include": ["${sourceDir}/CMakePresets.json"],
  "configurePresets": [
    {
      "name": "experiments-supermuc",
      "inherits": ["experiments"],
      "binaryDir": "build",
      "cacheVariables": {
        "FETCHCONTENT_FULLY_DISCONNECTED": "ON",
        "FETCHCONTENT_BASE_DIR": "${sourceDir}/_deps"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "experiments-supermuc",
      "configurePreset": "experiments-supermuc",
      "jobs": 32
    }
  ]
}
